{
    "body": {
        "installed": {
            "options": {
                "empty": "L_PLUGIN_INSTALLED_NO",
                "type": "grid",
                "image": false
            },
            "data": {
                "title": "L_PLUGIN_INSTALLED"
            },
            "body": {
                "default": {
                    "options": {
                        "type": "/Admin/Plugin"
                    },
                    "data": {
                        "convert": {
                            "id": "plugin_id",
                            "title": "name"
                        },
                        "button": {
                            "setup": {
                                "data": {
                                    "icon": "fas fa-cog",
                                    "href": "/setup/{id}/",
                                    "title": "L_MANAGE"
                                }
                            },
                            "uninstall": {
                                "data": {
                                    "icon": "fas fa-times",
                                    "ajax": "process-window",
                                    "title": "L_UNINSTALL"
                                }
                            }
                        }
                    }
                }
            }
        },
        "available": {
            "options": {
                "empty": "L_PLUGIN_LIST_NO",
                "type": "grid",
                "image": false
            },
            "data": {
                "title": "L_PLUGIN_LIST"
            },
            "body": {
                "default": {
                    "options": {
                        "type": "/Admin/Plugin"
                    },
                    "data": {
                        "convert": {
                            "id": "id",
                            "title": "name",
                            "desc": "desc"
                        },
                        "button": {
                            "install": {
                                "data": {
                                    "icon": "fas fa-download",
                                    "ajax": "process",
                                    "title": "L_INSTALL"
                                }
                            },
                            "delete": {
                                "data": {
                                    "title": "L_DELETE"
