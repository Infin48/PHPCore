{
    "body": {
        "log": {
            "options": {
                "empty": "L_LOG_LIST_NO"
            },
            "data": {
                "title": "L_LOG_LIST_LAST"
            },
            "body": {
                "default": {
                    "options": {
                        "template": {
                            "big": "/Log.phtml"
                        }
                    },
                    "data": {}
                }
            }
        },
        "news": {
            "options": {
                "empty": "L_FRONT_NEWS_NO"
            },
            "data": {
                "title": "L_FRONT_NEWS"
            },
            "body": {
                "default": {
                    "options": {
                        "template": {
                            "big": "/News.phtml"
                        }
                    },
                    "data": {
                        "convert": {
                            "url": "url"
                        },
                        "button": {
                            "info": {
                                "options": {
                                    "target": "_blank"
                                },
                                "data": {
                                    "href": "${url}"
                                }
                            }
                        }
                    }
                }
            }
        },
        "users": {
            "data": {
                "icon": "fas fa-user",
                "title": "L_FRONT_USER"
            },
            "body": {
                "default": {
                    "options": {
                        "template": {
                            "big": "/User.phtml"
                        }
                    },
                    "data": {
                        "convert": {
                            "id": "user_id"
                        },
                        "button": {
                            "edit": {
                                "data": {
                                    "href": "/user/show/{id}/"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}